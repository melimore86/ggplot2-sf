x=c(45.6,45.4),
y=c(21.7,19.8),
xend=c(58,58),
yend=c(35,12))
gg <-
gg +
geom_segment(data = df.arrows %>% filter(id==1),
aes(x=x,y=y,xend=xend,yend=yend),
arrow = arrow(type="closed",length = unit(0.25,"cm"))) +
geom_segment(data = df.arrows %>% filter(id==2),
aes(x=x,y=y,xend=xend,yend=yend),
arrow = arrow(type="closed",length = unit(0.25,"cm")))
}
